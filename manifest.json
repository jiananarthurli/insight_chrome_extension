{
  "name": "WikiGo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Event recommendation from Wikipedia browsing",
  "homepage_url": "https://go.aksingh.net/todoizr",
  "icons": {
  "16": "icons/icon16.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["src/bg/background.js"],
    "persistent": false
  },
  "default_locale": "en",
  "browser_action": {
  "default_icon": "icons/icon19_grey.png",
  "default_title": "WikiGo",
  "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
  "storage",
  "tabs",
  "webNavigation"
  ]
}